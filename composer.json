{
  "name": "honeystone/laravel-seo",
  "description": "SEO metadata and JSON-LD package for Laravel.",
  "license": "MIT",
  "keywords": [
    "honeystone",
    "seo",
    "metadata",
    "twitter",
    "opengraph",
    "json-ld",
    "laravel"
  ],
  "authors": [
    {
      "name": "George Palmer",
      "email": "george@honeystone.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://honeystone.com",
  "require": {
    "php": "^8.2",
    "ext-zip": "*",
    "illuminate/contracts": "^10.0|^11.0|^12.0",
    "spatie/laravel-package-tools": "^1.16.4"
  },
  "require-dev": {
    "laradumps/laradumps-core": "^2.0",
    "larastan/larastan": "^3.0",
    "laravel/pint": "^1.21",
    "nunomaduro/collision": "^8.0",
    "orchestra/testbench": "^10.0",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0",
    "phpstan/extension-installer": "^1.4",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpunit/phpunit": "^11.0",
    "spatie/schema-org": "^3.23"
  },
  "suggest": {
    "spatie/schema-org": "For generating JSON-LD schema using a fluent interface."
  },
  "autoload": {
    "psr-4": {
      "Honeystone\\Seo\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Honeystone\\Seo\\Tests\\": "tests",
      "Workbench\\App\\": "workbench/app/",
      "Workbench\\Database\\Factories\\": "workbench/database/factories/",
      "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "post-autoload-dump": [
      "@clear",
      "@prepare"
    ],
    "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
    "prepare": "@php vendor/bin/testbench package:discover --ansi",
    "build": "@php vendor/bin/testbench workbench:build --ansi",
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "@build",
      "@php vendor/bin/testbench serve"
    ],
    "lint": [
      "@php vendor/bin/phpstan analyse"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Honeystone\\Seo\\Providers\\SeoServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
